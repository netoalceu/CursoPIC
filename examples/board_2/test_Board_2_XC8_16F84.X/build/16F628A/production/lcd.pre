# 30 "lcd.c"
void lcd_wr(unsigned char val)
{
unsigned char i;

RB4=0;

for(i=0;i<8;i++)
{
RB3=((val>>(7-i))&0x01);
RB4=1;
RB4=0;
}
}

void lcd_cmd(unsigned char val)
{
RA0=1;
lcd_wr(val);
RB3=0;
atraso_ms(3);
RA0=0;
atraso_ms(3);
RA0=1;
}

void lcd_dat(unsigned char val)
{
RA0=1;
lcd_wr(val);
RB3=1;
atraso_ms(3);
RA0=0;
atraso_ms(3);
RA0=1;
}

void lcd_init(void)
{
RA0=0;
RB3=0;
atraso_ms(20);
RA0=1;

lcd_cmd(0x30);
atraso_ms(5);
lcd_cmd(0x30);
atraso_ms(1);
lcd_cmd(0x38);
lcd_cmd(0x08);
lcd_cmd(0x0F);
lcd_cmd(0x01);
lcd_cmd(0x38);
lcd_cmd(0x80);
}

void lcd_str(const char* str)
{
unsigned char i=0;

while(str[i])
{
lcd_dat(str[i]);
i++;
}
}

